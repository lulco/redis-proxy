{
	"name": "lulco/redis-proxy",
	"description": "Library for Redis driver abstraction",
	"keywords": ["PHP", "Redis", "Predis"],
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Michal Lulco",
			"email": "michal.lulco@gmail.com"
		}
	],
	"require": {
		"php": "^7.4 || ^8.0 <8.2"
	},
	"require-dev": {
		"predis/predis": "^1.1",
		"squizlabs/php_codesniffer": "^3.3",
		"phpunit/phpunit": "^9.5.20"
	},
	"suggest": {
		"ext-redis": "Native PHP extension for Redis connectivity.",
		"predis/predis": "Flexible and feature-complete PHP client library for Redis https://github.com/nrk/predis/wiki"
	},
	"autoload": {
		"classmap": ["src/"]
	},
	"autoload-dev": {
		"classmap": ["tests/"]
	}
}
